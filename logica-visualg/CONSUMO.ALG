Algoritmo "Consumo"

Var
   distancia, combustivel, media: real

Inicio

      escreva("Distancia percorrida: ")
      leia(distancia)
      escreva("Combustível gasto: ")
      leia(combustivel)
      
      media <- distancia / combustivel
      
      escreval("Consumo medo: ", media:8:3)


Fimalgoritmo